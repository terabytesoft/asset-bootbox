{
    "name": "terabytesoft/bootbox",
    "type": "library",
    "description": "Yii3 Assets Bootbox",
    "keywords": [
        "yii3",
        "assets",
        "bootbox",
        "free"
    ],
    "homepage": "https://github.com/terabytesoft/bootbox",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "terabytesoftw@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
		"codeception/codeception": "^3.0",
		"foxy/foxy": "^1.0",
		"hiqdev/composer-config-plugin": "^1.0@dev",
        "yiisoft/di": "^3.0@dev",
		"yiisoft/log": "^3.0@dev",
		"yiisoft/log-target-file": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/yii-debug": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {"TerabyteSoft\\Assets\\BootBox\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"TerabyteSoft\\Assets\\BootBox\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        },
        "config-plugin": {
            "defines": "config/defines.php",
            "params": "config/params.php",
            "common": "config/common.php",
            "codecept": [
                "$defines",
                "$params",
                "$common",
                "config/tests.php"
            ]
        }
	}
}
